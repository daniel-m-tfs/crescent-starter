# .env.example
# Arquivo de exemplo de variáveis de ambiente
# Copie para .env e ajuste os valores

# Servidor
APP_HOST=0.0.0.0
APP_PORT=8080
APP_ENV=development

# Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=crescent_db
DB_USER=crescent_user
DB_PASSWORD=your_secure_password_here

# Segurança
JWT_SECRET=your_jwt_secret_key_here
API_KEY=your_api_key_here

# CORS
CORS_ORIGIN=*
CORS_METHODS=GET,POST,PUT,PATCH,DELETE,OPTIONS

# Rate Limiting
RATE_LIMIT_ENABLED=false
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=60
